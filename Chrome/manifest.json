{
   "action": {
      "default_icon": "imageye128.png",
      "default_popup": "popup.html"
   },
   "background": {
      "service_worker": "fixR.js"
   },
   "browser_specific_settings": {
      "gecko": {
         "id": "imageye@marenauta.com",
         "strict_min_version": "53.0"
      }
   },
   "commands": {
      "_execute_action": {
         "suggested_key": {
            "default": "Ctrl+Shift+Y",
            "mac": "Command+Shift+Y"
         }
      }
   },
   "default_locale": "en",
   "description": "__MSG_appDesc__",
   "host_permissions": [ "http://*/*", "https://*/*", "\u003Call_urls>" ],
   "icons": {
      "128": "imageye128.png"
   },
   "manifest_version": 3,
   "name": "__MSG_appName__",
   "permissions": [ 
      "activeTab", 
      "downloads", 
      "storage", 
      "webRequest", 
      "scripting", 
      "declarativeNetRequest" 
   ],
   "short_name": "Imageye",
   "version": "3.1.1"
}
